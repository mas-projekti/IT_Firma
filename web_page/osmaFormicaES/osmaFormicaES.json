      "value" : [
        "if( $data.bestOfferData ){",
        "\treturn {",
        "\t\t//map bestOfferData variable to expected task contract input",
        "\t\tbestOfferDataInput: {",
        "\t\t\tproduct: $data.bestOfferData.product ? {",
        "\t\t\t\tpersistenceId_string: $data.bestOfferData.product.persistenceId_string !== undefined ? $data.bestOfferData.product.persistenceId_string : null",
