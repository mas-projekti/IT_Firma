        "\t\t\t\tpersistenceId_string: it.product.persistenceId_string !== undefined ? it.product.persistenceId_string : null,",
        "\t\t\t\ttypeName: it.product.typeName !== undefined ? it.product.typeName : null",
        "\t\t\t} : null,",
        "\t\t\tprice: it.price !== undefined ? it.price : null",
        "\t\t}))",
        "\t}",
        "}"
