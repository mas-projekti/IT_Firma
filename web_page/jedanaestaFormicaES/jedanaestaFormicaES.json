    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.zahtev && $data.zahtev_items && $data.zahtev_supplierOffers ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.zahtev.items = $data.zahtev_items;",
        "\t$data.zahtev.supplierOffers = $data.zahtev_supplierOffers;",
        "\treturn {",
        "\t\tbestOfferInput: $data.formInput.bestOfferInput,",
        "\t\t//map zahtev variable to expected task contract input",
        "\t\tzahtevInput: {",
        "\t\t\tsupplierOffers: $data.zahtev.supplierOffers.map( it => ({",
        "\t\t\t\tpersistenceId_string: it.persistenceId_string !== undefined ? it.persistenceId_string : null,",
        "\t\t\t\tproduct: it.product ? {",
        "\t\t\t\t\tpersistenceId_string: it.product.persistenceId_string !== undefined ? it.product.persistenceId_string : null,",
        "\t\t\t\t\ttypeName: it.product.typeName !== undefined ? it.product.typeName : null",
        "\t\t\t\t} : null,",
