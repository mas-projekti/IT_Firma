        "\t\t//map supplierOffers variable to expected task contract input",
        "\t\tsupplierOffersInput: $data.supplierOffers.map( it => ({",
        "\t\t\tpersistenceId_string: it.persistenceId_string !== undefined ? it.persistenceId_string : null,",
